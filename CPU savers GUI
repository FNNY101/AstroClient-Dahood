-- Function to create the CPU Saver GUI overlay
local function createOverlay()
    local player = game.Players.LocalPlayer
    local screenGui = nil
    local overlayEnabled = false -- Track whether the overlay is currently enabled or not

    -- Function to create the overlay and display text
    local function updateOverlayVisibility()
        if overlayEnabled then
            if screenGui then
                screenGui:Destroy()
                screenGui = nil
            end
        else
            screenGui = Instance.new("ScreenGui")
            screenGui.Name = "OverlayGui"
            screenGui.Parent = player:WaitForChild("PlayerGui")

            -- Create elements for the overlay
            -- Example: Create Frame, TextLabels, etc.

            -- Example: Keybind to toggle overlay
            game:GetService("UserInputService").InputBegan:Connect(function(input)
                if input.KeyCode == Enum.KeyCode.L then
                    overlayEnabled = not overlayEnabled
                    updateOverlayVisibility()
                end
            end)
        end
    end

    -- Initial call to create the overlay
    updateOverlayVisibility()
end

-- Load the CPU Saver GUI overlay using loadstring
loadstring(game:HttpGet("https://raw.githubusercontent.com/FNNY101/Astrohub-Dahood/main/CPU%20savers%20GUI"))()

